/* When the combo box is focused */
QComboBox:focus {
    border: 2px solid #00bfff; /* Bright blue border */
    background-color: #f0f8ff; /* Light blue background for visibility */
}

/* Drop-down button */
QComboBox::drop-down {
    border: none;
    background: transparent;
    width: 24px;
    padding-right: 6px;
}

/* Custom dropdown arrow */
QComboBox::down-arrow {
    image: url(arrow.png); /* Ensure this path is correct */
    width: 14px;
    height: 14px;
}

/* Drop-down list */
QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 2px solid #213555;
    selection-background-color: #00bfff; /* Bright blue selection */
    selection-color: #ffffff; /* White text for contrast */
    border-radius: 8px;
    padding: 5px;
    font-size: 13px;
}

/* Hover effect for list items */
QComboBox QAbstractItemView::item:hover {
    background-color: #0077cc; /* Slightly darker blue for hover */
    color: #ffffff;
}
